<Window Name="Main_Window"
        x:Class="POSSystem_Retail.ItemButtons"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:POSSystem_Retail"
        mc:Ignorable="d"
        Title="Items" Height="600" Width="800" SizeChanged="Main_Window_SizeChanged"
        WindowStartupLocation="CenterScreen"
        >
    <Window.Resources>
        <Style x:Name="scrollbar" x:Key="{x:Type ScrollBar}" TargetType="{x:Type ScrollBar}">
            <Setter Property="MinWidth" Value="65" />
            <Setter Property="Width" Value="65" />
        </Style>
    </Window.Resources>
    <Grid Name="MainGrid">
        
    </Grid>
</Window>
